{
    "model": {
        "type": "select",
        "property": "custom_model_data",
        "fallback": {
            "type": "model",
            "model": "item/wheat"
        },
        "cases": [
            {
                "when": "flour",
                "model": {
                    "type": "model",
                    "model": "item/flour"
                }
            },
            {
                "when": "dough",
                "model": {
                    "type": "model",
                    "model": "item/dough"
                }
            }
        ]
    }
}